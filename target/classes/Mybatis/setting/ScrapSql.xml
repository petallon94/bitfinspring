<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="scrap">
	<!--   scrap list 출력 -->
	<select id="selectAllOfScrap" resultType="sdto">
		select * from scrap order by num desc
	</select>
	<select id="totalCountOfScrap" resultType="int">
		select count(*) from scrap
	</select>
</mapper>